package com.molydeux {		import shapes.Box;	import flash.display.*;	import flash.events.*;		import com.molydeux.BabyModel;		import com.utils.InterfaceEvent;	import com.utils.ResizeUtil;		public class DoorDestroy extends Box{		private var _stage:Stage;				private var _levelOfReactivity:Boolean;				private var theDoorDestroyGFX:MovieClip;				public var doorOpened:Boolean = false;		public function DoorDestroy() {			_levelOfReactivity = false;			//			///*			// _stage = ResizeUtil.getInstance().stage;						stage.addEventListener(InterfaceEvent.SHRINKBABY, shrinkFX, false, 0, true);			stage.addEventListener(InterfaceEvent.SHAKEBABY, shakeFX, false, 0, true);			// */			//			addGFX();		}		///*		private function addGFX():void{			theDoorDestroyGFX = new door_destroy_gfx();			// theDoorDestroyGFX.width = theDoorDestroyGFX.height = 100;			addChild(theDoorDestroyGFX);					}		// */		private function shrinkFX(e:Event):void{					}		private function shakeFX(e:Event):void{					}						public function increaseReactivity():void {			_levelOfReactivity = true;						if(BabyModel.babyHealth <=2){								theDoorDestroyGFX.gotoAndStop("opened");				doorOpened=true;							} else if(doorOpened==false){				//Change animation				/// gotoAndStop(currentFrame + 1);				theDoorDestroyGFX.gotoAndStop("inactive");							}		}				public function removeReactivity():void {			_levelOfReactivity = false;			// gotoAndStop(currentFrame - 1);			if(doorOpened==false){				theDoorDestroyGFX.gotoAndStop("inactive");			} else {				theDoorDestroyGFX.gotoAndStop("active");			}		}				public function isReactive():Boolean {			return _levelOfReactivity; 		}				public function get levelOfReactivity():Boolean {			return _levelOfReactivity;		}				public function doZap():void{						dispatchEvent(new InterfaceEvent("zapAvatar"));		}				public function doDestroy():void{			trace("DESTROY");			this.destroy();					}		/// end class	}	/// end package	}